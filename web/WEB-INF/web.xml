    <env-entry>
        <env-entry-name>java.naming.provider.url</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>ldap://ldap.stackframe.com:389/dc=stackframe,dc=com</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>java.naming.security.authentication</env-entry-name>
